{% extends "partials/base.njk" %}

{% block meta %}
{% endblock %}
{% block content %}
    <style type="text/css">
        body{
            opacity: 0;
            -webkit-transition: all 1s ease-out;  
            -moz-transition: all 1s ease-out;  
            -o-transition: all 1s ease-out;  
            transition: all 1s ease-out;
        }
    </style>
    <div class="building-header">
            <div class="building-parallax cursor-pointer openGalleryModal" id="openGalleryModal" imageSrc="./img/photo532577139161512309.jpg">
                
            </div>
    </div>
    <div id="galleryModal" class="modal">
        <!-- Modal content -->
        <div class="gallery-modal-content cierralo-flex-row">
            <div class="modal-close-btn-container cursor-pointer">
                <span class="close cierralo-font-helveltica-neue-light cursor-pointer"><img src="./img/icons/close-modal.png" class="close-modal-img"></span>
            </div>
            <img src="" alt="" class="gallery-modal-image" id="galleryImage">
            <div class="arrow-icon-gallery-box left cursor-pointer cierralo-flex-row" id="arrow-left">
                <img src="./img/arrow-left.png" alt="" class="arrow-icon-gallery">
            </div>
            <div class="arrow-icon-gallery-box right cursor-pointer cierralo-flex-row" id="arrow-right">
                <img src="./img/arrow-right.png" alt="" class="arrow-icon-gallery">
            </div>
        </div>
    </div>
    <div class="building-main onload">
        <div class="galery-container cierralo-flex-row-flex-start">
            <div class="more-button-container cursor-pointer">
                <img src="./img/icons/more.png" alt="" class="more-icon-gallery">
            </div>
            <div id="carouselBox" class="cierralo-flex-row-flex-start">
                <img src="./img/photo532577139161512309.jpg" alt="" class="image-gallery openGalleryModal cursor-pointer" imageSrc="./img/photo532577139161512309.jpg">
                <img src="./img/casa_en_renta_miguel_hidalgo_ciudad_de_mexico_ciudad_de_mexico_8830079479412680177.jpg" alt="" class="image-gallery openGalleryModal cursor-pointer" imageSrc="./img/casa_en_renta_miguel_hidalgo_ciudad_de_mexico_ciudad_de_mexico_8830079479412680177.jpg">
                <img src="./img/N-B7-EU120716-3_Drupal Main Image.var_1468303653.jpg" alt="" class="image-gallery openGalleryModal cursor-pointer" imageSrc="./img/N-B7-EU120716-3_Drupal Main Image.var_1468303653.jpg">
                <img src="./img/s838x629_1473954798145_46696580.jpg" alt="" class="image-gallery openGalleryModal cursor-pointer" imageSrc="./img/s838x629_1473954798145_46696580.jpg">
            </div>
        </div>
        <div class="building-information-container cierralo-flex-row-flex-start">
            <div class="owner-information-container cierralo-flex-colum">
                <img src="./img/dude.png" alt="" class="owner-photo">
                <div class="owner-name-container">
                    <p class="role-name letter-spacing-dot-six cierralo-font-helveltica-medium cierralo-purplish-text">Propietario</p>
                    <p class="user-name letter-spacing-dot-six cierralo-font-helveltica-neue-light cierralo-greyish-two">José Martínez</p>
                </div>
                <div class="owner-contact-container">
                    <a href="./login.html" class="contact-link cursor-pointer"><p class="contact-link-text letter-spacing-dot-six cierralo-font-helveltica-neue-light cierralo-orange-text">Contáctalo</p></a>
                </div>
            </div>
            <div class="building-text-container">
                <div class="building-place-container">
                    <p class="building-street-text letter-spacing-dot-six cierralo-purplish-grey cierralo-font-helveltica-medium">Colima 228</p>
                    <p class="building-place-text letter-spacing-dot-six cierralo-purplish-grey cierralo-font-helveltica-neue-light">Roma Norte</p>
                    <div class="love-icon-container cursor-pointer">
                        <i class="fa fa-heart-o icon-love-detail" aria-hidden="true"></i>
                    </div>
                </div>
                <div class="building-detail-divider">
                    
                </div>
                <div class="building-price-container">
                    <p class="building-price-text letter-spacing-dot-six cierralo-font-helveltica-neue-light cierralo-orange-text">3.6 MDP</p>
                </div>
                <div class="building-full-address-container">
                    <p class="building-full-address-text letter-spacing-dot-six cierralo-pinkish-grey cierralo-font-helveltica-neue-light">Calle Colima No. 228 Colonia Roma Norte,
delegación Cuauhtémoc. C.P. 06700,
Ciudad de México.</p>
                </div>
                <div class="building-features-container cierralo-flex-row-flex-start">
                    <div class="building-single-feature-container">
                        <div class="building-feature-icon-box">
                            <img src="./img/icons/icon-house.png" alt="" class="feature-icon-house">
                        </div>
                        <p class="building-feature-text letter-spacing-dot-six cierralo-pinkish-grey cierralo-font-helveltica-neue-light">Casa</p>
                    </div>
                    <div class="building-single-feature-container">
                        <div class="building-feature-icon-box">
                            <img src="./img/icons/page-1@3x.png" alt="" class="feature-icon-rule">
                        </div>
                        <p class="building-feature-text letter-spacing-dot-six cierralo-pinkish-grey cierralo-font-helveltica-neue-light meters">m2</p>
                    </div>
                    <div class="building-single-feature-container">
                        <div class="building-feature-icon-box">
                            <img src="./img/icons/fill-1@3x.png" alt="" class="feature-icon-bed">
                        </div>
                        <p class="building-feature-text letter-spacing-dot-six cierralo-pinkish-grey cierralo-font-helveltica-neue-light rooms">2 cuartos</p>
                    </div>
                    <div class="building-single-feature-container">
                        <div class="building-feature-icon-box">
                            <img src="./img/icons/group-3@3x.png" alt="" class="feature-icon-shower">
                        </div>
                        <p class="building-feature-text letter-spacing-dot-six cierralo-pinkish-grey cierralo-font-helveltica-neue-light toilets">2 baños</p>
                    </div>
                    <div class="building-single-feature-container">
                        <div class="building-feature-icon-box">
                            <img src="./img/icons/icon-car.png" alt="" class="feature-icon-car">
                        </div>
                        <p class="building-feature-text letter-spacing-dot-six cierralo-pinkish-grey cierralo-font-helveltica-neue-light places">2 espacios</p>
                    </div>
                </div>
                <div class="building-buttons-container">
                    <a href="./login.html"><button class="cierralo-font-helveltica-neue-light letter-spacing-dot-six cierralo-make-offer-building-btn cierralo-btn-secondary cursor-pointer">Realizar oferta</button></a>
                    <a href="./login.html"><button class="cierralo-font-helveltica-neue-light letter-spacing-dot-six cierralo-date-building-btn cierralo-btn-secondary cursor-pointer">Agendar cita</button></a>
                </div>
            </div>
            <div class="building-share-container">
                <a href="" class="share-icon-link cursor-pointer">
                    <div class="share-icon-box">
                        <img src="./img/icons/facebook-logo-button@3x.png" alt="" class="building-share-icon">
                    </div>
                </a>
                <a href="" class="share-icon-link cursor-pointer">
                    <div class="share-icon-box">
                        <img src="./img/icons/twitter-logo-button@3x.png" alt="" class="building-share-icon">
                    </div>
                </a>
                <a href="" class="share-icon-link cursor-pointer">
                    <div class="share-icon-box">
                        <img src="./img/icons/mail-icon.png" alt="" class="building-share-icon">
                    </div>
                </a>
            </div>
        </div>
        <div class="building-body-divider">
                    
        </div>
        <div class="building-about-container">
            <p class="cierralo-grey-text letter-spacing-dot-six cierralo-font-helveltica-neue-light building-about-text">Enjoy a classic Los Angeles 1930's Spanish home with a private entrance, en-suite bathroom, and parking. Walk to LA's hottest restaurants and bars, The Grove outdoor mall, Melrose Place, 3rd St, the Beverly Center, and popular landmarks.</p>
        </div>
        <div class="building-services-container inline-block">
            <div class="services-container">
                <p class="service-title cierralo-pinkish-grey letter-spacing-dot-six cierralo-font-helveltica-medium">Servicios</p>
                <div class="service-box cierralo-flex-row">
                    <img src="./img/icons/gym-icon.png" alt="" class="building-service-icon inline-block">
                    <p class="building-service-text inline-block cierralo-pinkish-grey cierralo-font-helveltica-neue-light letter-spacing-dot-six">Gym</p>
                </div>
                <div class="service-box cierralo-flex-row">
                    <img src="./img/icons/wifi-icon.png" alt="" class="building-service-icon inline-block">
                    <p class="building-service-text inline-block cierralo-pinkish-grey cierralo-font-helveltica-neue-light letter-spacing-dot-six">Wifi</p>
                </div>
                <div class="service-box cierralo-flex-row">
                    <img src="./img/icons/flag@3x.png" alt="" class="building-service-icon inline-block">
                    <p class="building-service-text inline-block cierralo-pinkish-grey cierralo-font-helveltica-neue-light letter-spacing-dot-six">Circuito Cerrado</p>
                </div>
            </div>
            <div class="special-features-container">
                <p class="service-title cierralo-pinkish-grey letter-spacing-dot-six cierralo-font-helveltica-medium">Carac. Generales</p>
                <div class="service-box cierralo-flex-row">
                    <img src="./img/icons/pool-icon.png" alt="" class="building-service-icon inline-block">
                    <p class="building-service-text inline-block cierralo-pinkish-grey cierralo-font-helveltica-neue-light letter-spacing-dot-six">Alberca</p>
                </div>
                <div class="service-box cierralo-flex-row">
                    <img src="./img/icons/diamond@3x.png" alt="" class="building-service-icon inline-block">
                    <p class="building-service-text inline-block cierralo-pinkish-grey cierralo-font-helveltica-neue-light letter-spacing-dot-six">Acabados de lujo</p>
                </div>
                <div class="service-box cierralo-flex-row">
                    <img src="./img/icons/flower-icon.png" alt="" class="building-service-icon inline-block">
                    <p class="building-service-text inline-block cierralo-pinkish-grey cierralo-font-helveltica-neue-light letter-spacing-dot-six">Lobby</p>
                </div>
            </div>
            <div class="out-features-container">
                <p class="service-title cierralo-pinkish-grey letter-spacing-dot-six cierralo-font-helveltica-medium">Exteriores</p>
                <div class="service-box cierralo-flex-row">
                    <img src="./img/icons/waves-icon.png" alt="" class="building-service-icon inline-block">
                    <p class="building-service-text inline-block cierralo-pinkish-grey cierralo-font-helveltica-neue-light letter-spacing-dot-six">Carril de nado</p>
                </div>
                <div class="service-box cierralo-flex-row">
                    <img src="./img/icons/group-11@3x.png" alt="" class="building-service-icon inline-block">
                    <p class="building-service-text inline-block cierralo-pinkish-grey cierralo-font-helveltica-neue-light letter-spacing-dot-six">Cancha de Tennis</p>
                </div>
                <div class="service-box cierralo-flex-row">
                    <img src="./img/icons/group-8@3x.png" alt="" class="building-service-icon inline-block">
                    <p class="building-service-text inline-block cierralo-pinkish-grey cierralo-font-helveltica-neue-light letter-spacing-dot-six">Cancha de Paddel</p>
                </div>
            </div>
        </div>
        <div class="map-container inline-block" id="googleMap">
            <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCwzwWOG7rzZc7soDadgDWmVTpMnbMIQm4&sensor=false">
            </script>
            <script>
                var map;
                function initialize() {
                    var myLatLng = {lat: 19.4192741, lng: -99.1646228};
                    var mapOptions = {
                    zoom: 15,
                    center: new google.maps.LatLng(19.4192741,-99.1646228),
                    mapTypeId: google.maps.MapTypeId.ROADMAP,
                    scrollwheel: false
                    };
                    map = new google.maps.Map(document.getElementById('googleMap'),
                    mapOptions);
                    var marker = new google.maps.Marker({
                        position: myLatLng,
                        map: map,
                        title: 'Hello World!'
                    });
                }

                google.maps.event.addDomListener(window, 'load', initialize);
            </script>
        </div>
        <div class="building-body-divider">
                    
        </div>
    </div>
{% endblock %}
